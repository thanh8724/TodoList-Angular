{
  "name": "client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-css": "node-sass --include-path ./client/src --output-style compressed -o ./client/dist/app/assets/css ./client/src/app/assets/scss",
    "watch-css": "nodemon --watch ./client/src/app/assets/scss --ext scss --exec \"npm run build-css\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.10",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.10",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.9",
    "@ionic/angular": "^8.1.2",
    "@ngx-formly/core": "^6.3.3",
    "@ngx-formly/ng-zorro-antd": "^6.3.3",
    "ionicons": "^7.4.0",
    "node-sass": "^9.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.7",
    "@angular/compiler-cli": "^17.3.0",
    "@ionic/angular-toolkit": "latest",
    "@types/jasmine": "~5.1.0",
    "autoprefixer": "^10.4.19",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "nodemon": "^3.1.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "~5.4.2"
  }
}
